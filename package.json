{
  "name": "hinna-api-management-swagger",
  "version": "1.0.0",
  "description": "Centralized API documentation for Hinna microservices",
  "main": "index.js",
  "scripts": {
    "fetch-local": "node scripts/fetch-local-specs.js",
    "scan": "node scripts/scan-repository.js",
    "update": "node scripts/update-swagger.js",
    "list-repos": "node scripts/list-hinna-repos.js",
    "scan-all": "node scripts/scan-all-repositories.js",
    "validate": "swagger-cli validate public/swagger.yaml",
    "serve": "npx http-server public -p 8080",
    "test": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BK-Hinna-Work/Hinna-API-Management-Swagger.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "api-documentation",
    "hinna"
  ],
