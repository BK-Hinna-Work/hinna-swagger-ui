  "bugs": {
    "url": "https://github.com/BK-Hinna-Work/Hinna-API-Management-Swagger/issues"
  },
  "homepage": "https://github.com/BK-Hinna-Work/Hinna-API-Management-Swagger#readme",
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.2",
    "js-yaml": "^4.1.0"
  }
}
